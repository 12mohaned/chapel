
clean:
	rm -f $(CLEAN_TARGS)

cleandeps:
	rm -f $(CVS_DEPENDS)

clobber: 
	rm -rf ./$(CLOBBER_TARGS)

depend: $(PLATFORM_DIR)
	@echo Updating dependences...
	@echo > $(CVS_DEPENDS)
	@for file in $(CVS_SRCS); do \
	  $(ECHO) -n $(PLATFORM)/ >> $(CVS_DEPENDS); \
	  $(CCDEP) -MM -MG $(CFLAGS) $$file | grep -v ^# >> $(CVS_DEPENDS); \
	done
