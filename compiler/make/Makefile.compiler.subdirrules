
clean: $(CLEANSUBDIRS)
	rm -f $(CLEAN_TARGS)

cleandeps: $(CLEANSUBDIRDEPS)
	rm -f $(CVS_DEPENDS)

clobber: $(CLOBBERSUBDIRS)
	rm -rf ./$(CLOBBER_TARGS)

depend: $(DEPENDSUBDIRS) $(PLATFORM_DIR) $(DEPEND_DEPS)
	@echo Updating dependences...
	@echo > $(CVS_DEPENDS)
	@for file in $(CVS_SRCS); do \
	  $(ECHO) -n $(PLATFORM)/ >> $(CVS_DEPENDS); \
	  $(MAKEDEPEND) $(CFLAGS) $$file | grep -v \^\# >> $(CVS_DEPENDS); \
	done
	$(OTHER_DEPEND_RULES)
