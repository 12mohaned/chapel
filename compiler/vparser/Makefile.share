ifdef LANG_V
LANGS += v
CXXFLAGS += -DLANG_V=1
endif

VPARSER_SRCS = \
	ast_cf.cpp \
	grammar.cpp \
	parse.cpp \
	parse_ast.cpp \
	scope.cpp \

VPARSER_GEN_SRCS = \
	$(LANGS:%=%.g.d_parser.cpp)

CVS_SRCS = $(VPARSER_SRCS)
SRCS = $(CVS_SRCS)
GEN_SRCS = $(VPARSER_GEN_SRCS)

VPARSER_OBJS = \
	$(VPARSER_GEN_SRCS:%.cpp=$(VPARSER_OBJDIR)/%.o) \
	$(VPARSER_SRCS:%.cpp=$(VPARSER_OBJDIR)/%.o) \
