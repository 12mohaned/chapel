var m = 10;
var n = 5;
var answer = 0;

writeln("m = ", m);
writeln("n = ", n);
writeln("");

writeln("test: m << 1");
writeln("expected answer = 20");
answer = m << 1; 
writeln("actual answer = ", answer);
writeln("");

writeln("test: m >> 1");
writeln("expected answer = 5");
answer = m >> 1;
writeln("actual answer = ", answer);
writeln("");
 
writeln("test: n >> 1");
writeln("expected answer = 2");
answer = n >> 1;
writeln("actual answer = ", answer);
