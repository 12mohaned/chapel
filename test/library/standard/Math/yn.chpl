var a: real = 10, an: int = 5;
var b: real = 31, bn: int = 4;
var c: real = 0, cn: int = 7;
var d: real = 0.05, dn: int = 2;
var e: real = 100, en: int = 9;

writeln("a = ", a, " an = ", an);
writeln("yn(an, a) = ", yn(an, a));
writeln("b = ", b, " bn = ", bn);
writeln("yn(bn, b) = ", yn(bn, b));
writeln("c = ", c, " cn = ", cn);
writeln("yn(cn, c) = ", yn(cn, c));
writeln("d = ", d, " dn = ", dn);
writeln("yn(dn, d) = ", yn(dn, d));
writeln("e = ", e, " en = ", en);
writeln("yn(en, e) = ", yn(en, e));
