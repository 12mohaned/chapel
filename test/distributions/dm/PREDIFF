#!/bin/bash --norc

testname=$1
outputfile=$2
compiler=$3

# test-specific
case $testname in
  (t8) sed 's@\.chpl:[1-9][0-9]*:@.chpl:@' $outputfile > $outputfile.tmp && \
       mv $outputfile.tmp $outputfile ;;
esac

# sort the output
sort $outputfile > $outputfile.tmp
mv $outputfile.tmp $outputfile
