#! /usr/bin/env bash

la=$CHPL_HOME/third-party/hwloc/install/$($CHPL_HOME/util/printchplenv --make \
                                          | grep HWLOC_UNIQ \
                                          | cut -d'=' -f2)/lib/libhwloc.la
if grep -q '[-]lnuma' $la ; then
  cp $1.have-locality.good $1.good
else
  cp $1.no-locality.good $1.good
fi
