#! /usr/bin/env bash
COMM_SUB="$CHPL_COMM-$CHPL_COMM_SUBSTRATE"
if [ "$COMM_SUB" = "gasnet-udp" -a "$GASNET_SPAWNFN" = "L" ]; then
    echo "matches nodeName-nodeID" >$1.good
elif [ "$COMM_SUB" = "gasnet-smp" ]; then
    echo "matches nodeName-nodeID" >$1.good
else
    echo "matches nodeName" >$1.good
fi
