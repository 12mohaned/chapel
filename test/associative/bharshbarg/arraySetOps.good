foo = { a => 1, b => 2, c => 3, d => 4 }
bar = { c => 4, d => 4, e => 5, f => 6 }

Union: foo | bar = 
	{ a => 1, b => 2, c => 4, d => 4, e => 5, f => 6 }
Intersection: foo & bar = 
	{ c => 3, d => 4 }
Difference: foo - bar = 
	{ a => 1, b => 2 }
Xor: foo ^ bar = 
	{ a => 1, b => 2, e => 5, f => 6 }
