    } {
      Fl_Button {} {
        xywh {0 588 60 2} box NO_BOX
        code0 {o->clear_visible_focus();}
      }
      Fl_Button {} {
        image {Graph-2-40x40.png} xywh {10 100 40 40} box NO_BOX down_box FLAT_BOX selection_color 48
        code0 {o->clear_visible_focus();}
      }
      Fl_Button {} {
        image {Grid-40x40.png} xywh {10 150 40 40} box NO_BOX down_box FLAT_BOX selection_color 48
        code0 {o->clear_visible_focus();}
      }
    }
    Fl_Group DataField {open
      xywh {60 90 505 505} color 0 resizable
      class Fl_MultiGroup
    } {
      Fl_Group GraphGrp {open selected
        xywh {60 90 500 500} color 7
      } {
