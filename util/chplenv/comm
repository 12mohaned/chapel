#!/usr/bin/env perl

use FindBin;
use lib $FindBin::Bin;
use Run qw(ret run);
use strict;

my $preset_comm=$ENV{'CHPL_COMM'};
my $comm;

if ($preset_comm eq "") {
    # eventually, we'll want to pick defaults based on platforms, but for
    # today things are impoverished enough for us to want to pick "none"
    # by default
    $comm = "none";
} else {
    $comm = $preset_comm;
}


ret($comm);
print "$comm\n" unless caller;
